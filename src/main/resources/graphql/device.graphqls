type Device {
  id: String!
  name: String!
  data: Data
}

type Data {
  generation: String
  capacityGB: Int
  capacity: String
  color: String
  description: String
  colour: String
  caseSize: String
  year: Int
  price: Float
  CPUModel: String
  hardDiskSize: String
}

# Root
